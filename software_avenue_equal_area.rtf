\par \hich\af1\dbch\af31505\loch\f1 end
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 ' Establish a lower limit of search. '
\par \hich\af1\dbch\af31505\loch\f1 xRadiusLow = (xAreaTarget / xAreaUnitCircle).Sqrt xAreaLow =
\par \hich\af1\dbch\af31505\loch\f1 xAreaTarget
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 ' Establish the initial estimate.
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 xRadius = xRadiusLow
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 '==> DEBUGGING
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 s = "Target area:" ++ xAreaTarget.AsString s = s + NL +
\par \hich\af1\dbch\af31505\loch\f1 "Iteration, radiusLow, areaLow, radius, area, radiusHigh, areaHigh"
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 ' Search for the buffer.
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 nIter = 0
\par \hich\af1\dbch\af31505\loch\f1 while (nIter < nIterMax)
\par \hich\af1\dbch\af31505\loch\f1 nIter = 1 + nIter
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 ' Compute area of current estimate.
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 plyCircle \hich\af1\dbch\af31505\loch\f1 = Circle.Make(ptOrigin, xRadius).AsPolygon plyBuffer =
\par \hich\af1\dbch\af31505\loch\f1 plyRegion.ReturnIntersection(plyCircle) xArea = plyBuffer.ReturnArea
\par \hich\af1\dbch\af31505\loch\f1 if (xArea.IsNull) then xArea = 0 end '
\par \hich\af1\dbch\af31505\loch\f1 '==> DEBUGGING
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 s = s + NL + NL + niter.AsString ++
