\par \hich\af1\dbch\af31505\loch\f1 xArea.AsString ++ xRadiusHigh.AsString ++ xAreaHigh.AsString '
\par \hich\af1\dbch\af31505\loch\f1 ' Check for termination.
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 if ((xRadiusHigh - xRadiusLow)/(xRadiusHigh + xRadiusLow)
\par \hich\af1\dbch\af31505\loch\f1 <= xTolerance) then
\par \hich\af1\dbch\af31505\loch\f1 break
\par \hich\af1\dbch\af31505\loch\f1 end
\par \hich\af1\dbch\af31505\loch\f1 if (((xArea - xAreaTarget)/(xArea + xAreaTarget)).Abs
\par \hich\af1\dbch\af31505\loch\f1 <= xAreaTolerance) then
\par \hich\af1\dbch\af31505\loch\f1 break
\par \hich\af1\dbch\af31505\loch\f1 end
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 ' Update the estimate range [xRadiusLow, xRadiusHigh] '
\par \hich\af1\dbch\af31505\loch\f1 if (xArea > xAreaTarget) then
\par \hich\af1\dbch\af31505\loch\f1 xAreaHigh = xArea
\par \hich\af1\dbch\af31505\loch\f1 xRadiusHigh = xRadius
\par \hich\af1\dbch\af31505\loch\f1 else
\par \hich\af1\dbch\af31505\loch\f1 xAreaLow = xArea
\par \hich\af1\dbch\af31505\loch\f1 xRadiusLow = xRadius
\par \hich\af1\dbch\af31505\loch\f1 end
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 ' Use linear interpolation to estimate the ne\hich\af1\dbch\af31505\loch\f1 xt radius. '
\par \hich\af1\dbch\af31505\loch\f1 xRadiusNew = (xRadiusHigh - xRadiusLow) /
\par \hich\af1\dbch\af31505\loch\f1 (xAreaHigh.Sqrt - xAreaLow.Sqrt) *
\par \hich\af1\dbch\af31505\loch\f1 (xAreaTarget.Sqrt - xAreaLow.Sqrt) + xRadiusLow
\par \hich\af1\dbch\af31505\loch\f1 '
\par \hich\af1\dbch\af31505\loch\f1 ' Fall back to bisection if linear interpolation stagnates. '
